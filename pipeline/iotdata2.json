{
	"name": "iotdata2",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['messageId']"
								},
								"sink": {
									"name": "messageId",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['deviceId']"
								},
								"sink": {
									"name": "deviceId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['temperature']"
								},
								"sink": {
									"name": "temperature",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "$['humidity']"
								},
								"sink": {
									"name": "humidity",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "$['EventProcessedUtcTime']"
								},
								"sink": {
									"name": "EventProcessedUtcTime",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ds_iotrawdata",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "iot_csv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2023-01-22T06:57:47Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}